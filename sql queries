SELECT skills FROM SKILLS
WHERE id IN(  (SELECT UNNEST(skills) FROM project_traits where id =1));

"SELECT project_traits.id, project_traits.title, project_traits.description, 
(SELECT ARRAY(SELECT skill FROM SKILLS
WHERE id IN(  (SELECT UNNEST(skills) FROM project_traits where id = (SELECT id FROM project_traits WHERE title = "+ term + "))))
) AS skills, 
(SELECT ARRAY(SELECT interests FROM interests
WHERE id IN(
SELECT UNNEST(interests) FROM project_traits where id = (SELECT id FROM project_traits WHERE title = "+ term + "))))
FROM project_traits
WHERE id= (SELECT id FROM project_traits WHERE title = "+ term + ");"


SELECT project_traits.id, project_traits.title, project_traits.description, (SELECT ARRAY(SELECT skill FROM SKILLS WHERE id IN(  (SELECT UNNEST(skills) FROM project_traits where id = (SELECT id FROM project_traits WHERE title = 'Blah'))))) AS skills, (SELECT ARRAY(SELECT interests FROM interests WHERE id IN( SELECT UNNEST(interests) FROM project_traits where id = (SELECT id FROM project_traits WHERE title = "+ term + ")))) FROM project_traits WHERE id= (SELECT id FROM project_traits WHERE title = "+ term + ");"

************ EXACT MATCH *************
SELECT id FROM project_traits
WHERE 'term' = title
AND ARRAYskills = skills
AND ARRAYinterests = interests;

*********** NO CASE *****************
SELECT id FROM project_traits
WHERE LOWER('term') = LOWER(title)
AND ARRAYskills = skills
AND ARRAYinterests = interests;

*********** EXACT ON TAGS ************
SELECT id FROM project_traits
WHERE ARRAYskills = skills;

SELECT id FROM project_traits
WHERE ARRAYinterests = interests;

*********** TAGS CONTAINED BY PROJECT **************
SELECT id FROM project_traits
WHERE ARRAYskills <@ skills;

SELECT id FROM project_traits
WHERE ARRAYinterests <@ interests;

*********** PROJECT CONTAINS TAGS ****************
SELECT id FROM project_traits
WHERE ARRAYskills @> skills;

SELECT id FROM project_traits
WHERE ARRAYinterests @> interests;

*********** SEARCH IS SUBSET OF TITLE ************
SELECT id FROM project_traits
WHERE string_to_array(lower(term), ' ') <@ string_to_array(LOWER(title), ' ');

//search array will need parse and toLower first

*********** SEARCH IS SUBSET OF DESC *************
SELECT id FROM project_traits
WHERE ARRAYsearcharray <@ string_to_array(LOWER(title), ' ');

//search array will need parsing and toLowering first

